%% Coupling Inputs
NcycStart = 0;
outerCorrTolerance = 1e-5;
nOuterCorrMax = 200;
couplingScheme = 'Aitken';
%couplingScheme = 'FixedRelaxation';
fixedPtRelax    = 0.01     ; % Fixed-point displacement relaxation value

%% Fluid Solver Inputs
%%%% Free-Swimming %%%%%%%%%%%%%%%%%%%%%%%%%
f = 1.0;
A_c = 0.38;
Qinf = 1.0;
c = 1;
A_bp = 1;
M_star = 1/5;
Npanels = 300;
Nstep = 150;
Ncyc = 4;
Nlump = 4;
val = 2.5e-4;
TEfac = 0.4;

%% Structural Solver Inputs
flexionRatio 	= 0.30		       ; % Flexion Ratio
tConst 		= 0.95		       ; % After this percent of the meanline, beam thickness is treated as constant
elementFactor   = 2                    ; % Multiple of 20 for the number of structural elements
alpha           = 0.1		       ;
beta		= (1+alpha)^2/4        ;
gamma		= 0.5+alpha            ;
fracDeltaT      = 0.1                  ;

material        = 'Silicon-Rubber' ; % Name for the material
materialDensity = 1700             ; % The density of the material
YoungMod        = 25.5e6           ; % Young's Modulus of the linear, isotropic material
PoissonRat      = 0.00             ; % Poisson's ratio for the linear, isotropic material

% ABAQUS Specific (depreciated)
jobName         = 'ABAQUS-FSI'         ; % Name for the ABAQUS job
workDir         = pwd                  ; % Working directory for the ABAQUS calculations
cpus            = '1'                  ; % Number of CPUs to use. Must be a string
solverTol       = 1e-9                 ; % Residual tolerance for the structural solver
elemType        = 'B21H'               ; % Element type for the ABAQUS solver
application     = 'TRANSIENT FIDELITY' ; % Dampening level for the implicit structural solver

%% Settings:
% General Settings
runTimeModifiable = 1;				% 0 - off, 1 - on.

% Fluid Solver Settings
LES = 0;                                        % 0 - off, 1 - on. 
SC = 0;                                         % 0 - off, 1 - on. 
Rollup = 1;                                     % 0 - off, 1 - on.
Flowfield = 0;                                  % 0 - off, 1 - on.
wakeinf = 1;                                    % 0 - off, 1 - on.
bod = 0;                                        % 0 - off, 1 - on.
grd = 0;                                        % 0 - off, 1 - on.
free = 0;                                       % 0 - off, 1 - on.
SurfaceColor = 0;                               % 0 - shear stress, 1 - pressure
ViscDrag = 0;                                   % 0 - off, 1 - on.
PlotTimeStepFig = 1;                            % 0 - off, 1 - on.
PrintFigures = 1;                               % 0 - off, 1 - on.
PlotVelFig = 0;                                 % 0 - off, 1 - on.
SaveData = 0;                                   % 0 - off, 1 - on.
BodDrag = 1;                                    % 0 - off, 1 - on.
rampedStart = 1;                                % 0 - off, 1 - on.

% Structual Solver Settings
fileCleanup = 1;                                % 1 - generated files will be deleted 0 - save all files
interpMtd = 1;                                  % 1 - linear interpolation 0 - cubic spline interpolaation
solverType = 0;                                 % 1 - abaqus/explicit 0 - abaqus/standard (implicit)
debugPlots = 0;					% 0 - off, 1 - on.
constThickBeam = 1;				% 0 - off, 1 - on.
